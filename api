local BotAPI = {}

local vim = game:GetService('VirtualInputManager')
function BotAPI:Print(obj)
    print(obj)
end

function BotAPI:GetBot()
    return game.Players.LocalPlayer
end


function BotAPI:GetBotChar()
    return game.Players.LocalPlayer.Character
end
function BotAPI:LeaderCheck()
    if GetBotIndex() == 1 or "1" then return true end
end

function BotAPI:GetBotIndex()

  
    if game.Players.LocalPlayer.Name == alts.alt1 then
        return "1"
    elseif game.Players.LocalPlayer.Name == alts.alt2 then
        return "2"
    elseif game.Players.LocalPlayer.Name == alts.alt3 then
        return "3"
    elseif game.Players.LocalPlayer.Name == alts.alt4 then
        return "4"
    elseif game.Players.LocalPlayer.Name == alts.alt5 then
        return "5"
    elseif game.Players.LocalPlayer.Name == alts.alt6 then
        return "6"
    elseif game.Players.LocalPlayer.Name == alts.alt7 then
        return "7"
    elseif game.Players.LocalPlayer.Name == alts.alt8 then
        return "8"
    elseif game.Players.LocalPlayer.Name == alts.alt9 then
        return "9"
    elseif game.Players.LocalPlayer.Name == alts.alt10 then
        return "10"
    elseif game.Players.LocalPlayer.Name == alts.alt11 then
        return "11"
    elseif game.Players.LocalPlayer.Name == alts.alt12 then
        return "12"
    elseif game.Players.LocalPlayer.Name == alts.alt13 then
        return "13"
    elseif game.Players.LocalPlayer.Name == alts.alt14 then
        return "14"
    elseif game.Players.LocalPlayer.Name == alts.alt15 then
        return "15"
    elseif game.Players.LocalPlayer.Name == alts.alt16 then
        return "16"
    else
        return "BOT HAS NO INDEX"
    end
end

function BotAPI:Chat(msg)
    game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(msg, "All")
end

function BotAPI:Execute(msg)
    local func = loadstring(msg)
    func()
end

function BotAPI:Goto(location)
    local rootPart = BotAPI:GetBotChar().HumanoidRootPart
    rootPart.CFrame = CFrame.new(location)
end

function BotAPI:Kill()
    local botChar = BotAPI:GetBotChar()
    botChar.Humanoid.Health = 0
end
function BotAPI:HoldKey(key, time)
    vim:SendKeyEvent(true, key, false, nil)
    task.wait(time)
    vim:SendKeyEvent(false, key, false, nil)
end
function BotAPI:PressKey(key)
    vim:SendKeyEvent(true, key, false, nil)
    task.wait(0.005)
    vim:SendKeyEvent(false, key, false, nil)
end
function BotAPI:Add(num1,num2)
    return(num1 + num2)
end
function BotAPI:Multiply(num1,num2)
    return(num1 * num2)
end
function BotAPI:Divide(num1,num2)
    return(num1 / num2)
end
function BotAPI:Shutdown()
    Socket:Send("ShutdownPC")
end
function BotAPI:SendKey()
    Socket:Send("ShutdownPC")
end
function BotAPI:Kick(botid,reason)
    local var = tonumber(BotAPI:GetBotIndex())
  
    if var == botid or botid == "ALL" or botid == "all" or botid == "All" then
        BotAPI:GetBot():Kick("Bot API Requested Kick"..reason)
    else 
        print("Nigga Got Kicked LLLLL")
        print(var)
    end
end

return BotAPI
